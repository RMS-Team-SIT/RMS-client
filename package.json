{
  "name": "vue3-template",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "build:local": "vite build --mode localdev",
    "build:dev": "vite build --mode dev",
    "build:prod": "vite build --mode prod",
    "dev": "vite --host --mode dev",
    "local": "vite --host --mode localdev",
    "prod": "vite --host --mode prod",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:fix": "pnpm run lint:js -- --fix && pnpm run lint:style -- --fix",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "serve": "vite preview"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.18",
    "@kyvg/vue3-notification": "^3.0.2",
    "@sweetalert2/themes": "^5.0.15",
    "@tato30/vue-pdf": "^1.8.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.7.2",
    "chart.js": "^4.4.1",
    "dayjs": "^1.11.10",
    "firebase": "^10.8.0",
    "pinia": "^2.1.7",
    "sweetalert2": "^11.10.1",
    "theme-change": "^2.5.0",
    "vue": "^3.2.47",
    "vue-chartjs": "^5.3.0",
    "vue-countup-v3": "^1.4.1",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.1.6",
    "vue-sweetalert2": "^5.0.5",
    "vue3-easy-data-table": "^1.5.47",
    "vuefire": "^3.1.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "daisyui": "^4.0.9",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.4",
    "vite-svg-loader": "^4.0.0"
  }
}
